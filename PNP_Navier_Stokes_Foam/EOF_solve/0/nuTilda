FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet          
    {
        type            zeroGradient;
    }

    micropore_upperwall      
    {
        type            zeroGradient;
    }

    nanopore_upperwall       
    {
        type            zeroGradient;
    }

micropore_side_upperwall  
    {
        type            zeroGradient;
    }
  axis
    {
        type            empty;
    }
   
   
     front
    {
        type            wedge;
    }

   back
    {
        type            wedge;
    }
}

