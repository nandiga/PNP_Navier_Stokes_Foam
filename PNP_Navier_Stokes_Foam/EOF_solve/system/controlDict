FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application Diode_Foam;

startFrom       latestTime;

stopAt          endTime;

endTime         1e7;

deltaT          0.005;

writeControl    adjustableRunTime;

writeInterval   1e5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

